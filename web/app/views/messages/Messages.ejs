<% include ../inc/header %>


<div class="ui grid">

  <div class="three wide column"><p></p></div>

  <div class="twelve wide column">

    <div class="ui blue segment">
      <h1 class="ui header"><%= title %></h1>


        <div class="ui celled list">
          <% messages.forEach(function(message){ %>
            <div class="item">
              <img class="ui avatar image" src="/images/avatar/small/helen.jpg">
              <div class="content">
                <div class="header"><%= message.titre %></div>
                <%= message.dateCreation.getDate()+"/"+ (message.dateCreation.getMonth()+1)  +"/"+ message.dateCreation.getFullYear() +"  "+ message.dateCreation.getHours()+":"+message.dateCreation.getMinutes() %>
              </div>
            </div>
          <% }); %>
        </div>
    </div>
  </div>
</div>

<% include ../inc//footer %>
